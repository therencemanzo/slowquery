    "homepage": "https://github.com/therencemanzo/slowquery",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SlowQueryLogger": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
